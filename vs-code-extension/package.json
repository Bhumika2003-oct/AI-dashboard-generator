{
  "name": "ai-dashboard-generator",
  "displayName": "AI Dashboard Generator",
  "description": "Generate beautiful dashboards from your data using natural language",
  "version": "0.1.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCommand:ai-dashboard-generator.generateDashboard",
    "onView:aiDashboardGeneratorView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-dashboard-generator.generateDashboard",
        "title": "Generate Dashboard from Data"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-dashboard-generator",
          "title": "AI Dashboard Generator",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "ai-dashboard-generator": [
        {
          "id": "aiDashboardGeneratorView",
          "name": "Dashboard Generator"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "command": "ai-dashboard-generator.generateDashboard",
          "group": "navigation",
          "when": "resourceExtname == .csv || resourceExtname == .xlsx || resourceExtname == .xls"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "pandas-js": "^0.2.4",
    "plotly.js": "^2.12.0"
  }
} 